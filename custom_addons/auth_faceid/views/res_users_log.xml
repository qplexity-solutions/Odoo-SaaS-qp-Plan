<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

     <record id="view_tree_faceid_logs" model="ir.ui.view">
        <field name="name">tree.faceid</field>
        <field name="model">res.users.faceid.log</field>
        <field name="arch" type="xml">
          <tree string="Logs">
             <field name="res_user_id"/>
             <!-- <field name="descriptor"/> -->
             <field name="origin" widget="image"/>
             <field name="face" widget="image"/>
          </tree>
        </field>
    </record>

    <record id="view_form_faceid_logs" model="ir.ui.view">
        <field name="name">form.faceid</field>
        <field name="model">res.users.faceid.log</field>
        <field name="arch" type="xml">
          <form string="Face table">
            <sheet>
                <field name="face" widget="image" readonly="0"/>
                <field name="origin" widget="image" readonly="0"/>
              <group>
                <field name="res_user_id"/>
                <field name="descriptor"/>
              </group>
            </sheet>
          </form>
        </field>
    </record>

    <record id="auth_faceid_action_logs" model="ir.actions.act_window">
        <field name="name">Logs</field>
        <field name="res_model">res.users.faceid.log</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_empty_folder">
              No Logs found
            </p>
            <p>
              The logs faceid of your company will be displayed here.
            </p>
        </field>
    </record>

      <menuitem name="Logs"
          id="auth_faceid.menu_faceid_log_table"
          parent="auth_faceid.menu_root"
          groups="base.group_erp_manager"
          action="auth_faceid_action_logs"/>
  </data>
</odoo>